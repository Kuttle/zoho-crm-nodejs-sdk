{
  "name": "zoho-crm-nodejs-sdk",
  "version": "1.0.0",
  "description": "allows the oauth save load functions to be passed in as an object, not as a module",
  "main": "dist/ZCRMRestClient",
  "types": "dist/ZCRMRestClient.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build": "tsc",
    "fresh": "rimraf ./node_modules package-lock.json && npm install",
    "carpe-diem": "npm run fresh && npm run build",
    "release:patch": "npm version patch && git push --tags",
    "release:minor": "npm version minor && git push --tags",
    "release:major": "npm version major && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kuttle/zoho-crm-nodejs-sdk.git"
  },
  "keywords": [
    "zoho",
    "sdk",
    "nodejs",
    "typescript"
  ],
  "author": "Edward Sutton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kuttle/zoho-crm-nodejs-sdk/issues"
  },
  "homepage": "https://github.com/Kuttle/zoho-crm-nodejs-sdk#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "properties-reader": "0.0.16",
    "querystring": "^0.2.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.7",
    "@types/request": "^2.47.1",
    "typescript": "^3.0.1"
  }
}
